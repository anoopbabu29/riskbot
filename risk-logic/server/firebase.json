{
  "hosting": {
    "public": "src/dist/risk-ui",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html",
        "run": {
          "serviceId": "riskbot"
        }
      }
    ]
  }
}
